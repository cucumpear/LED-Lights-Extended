<Buildings>
 
    <!-- ======================Base========================= -->

  <ThingDef Abstract="True" Name="CCP_BaseLEDLight" ParentName="BuildingBase">
    <thingClass>Building</thingClass>
    <category>Building</category>
	<minifiedDef>MinifiedFurniture</minifiedDef>
	<graphicData>
       <texPath>Lights/LED_Lamp</texPath>
       <graphicClass>Graphic_Single</graphicClass>
       <shaderType>Transparent</shaderType>
    </graphicData>
    <blueprintGraphicData>
    <texPath>Lights/LED_Lamp</texPath>
    </blueprintGraphicData>
    <uiIconPath>Lights/LED_Lamp_MenuIcon</uiIconPath>
    <building>
	 <isInert>true</isInert>
      <ignoreNeedsPower>false</ignoreNeedsPower>
      <isEdifice>false</isEdifice>
    </building>
    <costList>
      <Steel>5</Steel>
      <Silver>25</Silver>
    </costList>
	<altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>Standable</passability>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
	<rotatable>false</rotatable>
    <selectable>true</selectable>
	<soundImpactDefault>BulletImpactMetal</soundImpactDefault>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
	<constructEffect>ConstructMetal</constructEffect>
    <statBases>
      <MaxHitPoints>35</MaxHitPoints>
      <WorkToBuild>80</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>2</Beauty>
    </statBases>
    <DesignationCategory>LEDLights</DesignationCategory>
    <comps>
      <li Class="CompProperties_Flickable"/>
    </comps>
    <researchPrerequisites>
      <li>CCP_LEDLights</li>
    </researchPrerequisites>
  </ThingDef>

 
