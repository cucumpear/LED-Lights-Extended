   <ThingDef ParentName="CCP_LEDRopeLightBase">
    <defName>CCP_FloorLight</defName>
    <label>LED floor light</label>
    <description>Power-saving LED light. Embedded in the floor and invisible. Acts as a conduit.</description>
	<graphicData>
       <texPath>LEDFloor_Lamp</texPath>
       <graphicClass>Graphic_Single</graphicClass>
       <shaderType>Transparent</shaderType>
    </graphicData>
    <uiIconPath>RopeLight_MenuIcon</uiIconPath>
    <statBases>
      <MaxHitPoints>20</MaxHitPoints>
      <WorkToBuild>50</WorkToBuild>
      <Flammability>0.5</Flammability>
    </statBases>
    <costList>
      <Steel>2</Steel>
      <Silver>5</Silver>
    </costList>
	<altitudeLayer>FloorEmplacement</altitudeLayer>
    <comps>
      <li Class="CompProperties_Power">
        <transmitsPower>true</transmitsPower>
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>1</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>3</glowRadius>
        <glowColor>(217,217,208,0)</glowColor>
      </li>
    </comps>
  </ThingDef>
  
